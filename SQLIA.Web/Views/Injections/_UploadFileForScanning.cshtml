@model SQLIA.Web.Models.UploadForScanView

@using (Html.BeginForm("RunScan", "Injections", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-inline" }))
{
    <div class="form-group">
        @Html.TextBoxFor(x => x.File, new { type = "file" , required="required"})
    </div>
      
   
    <div class="form-group">
        <button type="submit" class="btn btn-primary btn-sm">upload & scan</button>
    </div>
  
}